{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfnycpayroll-kashish-g"
		},
		"DataLake_Link_NYC_Payroll_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DataLake_Link_NYC_Payroll'"
		},
		"SQL_Link_NYC_Payroll_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SQL_Link_NYC_Payroll'"
		},
		"DataLake_Link_NYC_Payroll_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagenycpayroll1.dfs.core.windows.net/"
		},
		"SQL_Link_NYC_Payroll_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "server-nyc-payroll.database.windows.net"
		},
		"SQL_Link_NYC_Payroll_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db-nyc-paroll"
		},
		"SQL_Link_NYC_Payroll_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "serveradmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfnycpayroll-kashish-g_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DataLake_Link_NYC_Payroll_accountKey": {
						"value": "[parameters('DataLake_Link_NYC_Payroll_accountKey')]"
					},
					"SQL_Link_NYC_Payroll_password": {
						"value": "[parameters('SQL_Link_NYC_Payroll_password')]"
					},
					"DataLake_Link_NYC_Payroll_properties_typeProperties_url": {
						"value": "[parameters('DataLake_Link_NYC_Payroll_properties_typeProperties_url')]"
					},
					"SQL_Link_NYC_Payroll_properties_typeProperties_server": {
						"value": "[parameters('SQL_Link_NYC_Payroll_properties_typeProperties_server')]"
					},
					"SQL_Link_NYC_Payroll_properties_typeProperties_database": {
						"value": "[parameters('SQL_Link_NYC_Payroll_properties_typeProperties_database')]"
					},
					"SQL_Link_NYC_Payroll_properties_typeProperties_userName": {
						"value": "[parameters('SQL_Link_NYC_Payroll_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adfnycpayroll-kashish-g_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adfnycpayroll-kashish-g_ArmTemplate_0')]"
			]
		}
	]
}